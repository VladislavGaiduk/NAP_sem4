<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.LineChart?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.layout.VBox?>
<VBox xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/17"
      fx:controller="com.example.gui.controllers.ReportsController" spacing="10" alignment="CENTER">

    <LineChart fx:id="salaryChart" title="График выплат зарплаты">
        <xAxis>
            <NumberAxis label="Месяц"/>
        </xAxis>
        <yAxis>
            <NumberAxis label="Сумма (руб.)"/>
        </yAxis>
    </LineChart>


</VBox>